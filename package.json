{
  "name": "fatejs",
  "description": "Patterns & Guards & Joins, Oh My!",
  "author": "Thomas S. Bradford",
  "version": "0.9.0",
  "license": "MIT",
  "homepage": "http://www.fate-lang.org/",
  "keywords": [],
  "contributors": [
    {
      "name": "Thom Bradford",
      "email": "thom@kode4food.it"
    }
  ],
  "repository": "git@github.com/kode4food/fate-lang.git",
  "bin": {
    "fate": "bin/fate.js",
    "fatec": "bin/fatec.js"
  },
  "scripts": {
    "prepare": "mkdir -p dist/compiler && mkdir -p dist/modules && cp src/modules/*.fate dist/modules",
    "pegjs": "npm run prepare && pegjs -o dist/compiler/parser.js src/compiler/parser.pegjs",
    "build": "npm run pegjs && babel src -d dist",
    "test": "npm run build && nodeunit test/index.js"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "glob": "^7.1.3",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.80.0",
    "nodeunit": "^0.10.2",
    "pegjs": "^0.10.0"
  }
}
