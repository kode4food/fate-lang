{
  "name": "fatejs",
  "description": "Patterns & Guards & Joins, Oh My!",
  "author": "Thomas S. Bradford",
  "version": "0.9.0",
  "license": "MIT",
  "homepage": "http://www.fate-lang.org/",
  "keywords": [],
  "contributors": [
    {
      "name": "Thom Bradford",
      "email": "thom@kode4food.it"
    }
  ],
  "repository": "git@github.com/kode4food/fate-lang.git",
  "bin": {
    "fate": "bin/fate.js",
    "fatec": "bin/fatec.js"
  },
  "scripts": {
    "prepare": "mkdir -p dist/compiler && mkdir -p dist/modules && cp src/modules/*.fate dist/modules",
    "pegjs": "npm run prepare && pegjs -o dist/compiler/parser.js src/compiler/parser.pegjs",
    "lint": "eslint src",
    "babel": "babel src -d dist",
    "nodeunit": "nodeunit test/index.js",
    "build": "npm run pegjs && npm run babel",
    "test": "npm run build && npm run nodeunit"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "glob": "^7.1.3",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.80.0",
    "nodeunit": "^0.10.2",
    "pegjs": "^0.10.0"
  }
}
