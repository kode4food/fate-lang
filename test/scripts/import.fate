def explicit(test)
  import './basics' as basics
  
  basics.relational | test.ok
  test.done()
end

def multiple(test)
  import './basics' as basics1
  import './basics' as basics2
  
  test.equal(basics1, basics2)
  test.done()
end

def parent(test)
  import '../module1' as module1
  
  test.equal(module1.test_value, 'right!')
  test.done()
end

def parentFrom(test)
  from '../module1' import test_value as testv
  
  test.equal(testv, 'right!')
  test.done()
end

export explicit, multiple, parent, parentFrom

