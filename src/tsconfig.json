{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "removeComments": false,
    "outDir": "../dist",
    "sourceMap": true,
    "noImplicitAny": true,
    "noEmitOnError": false,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noImplicitUseStrict": true
  }
}
